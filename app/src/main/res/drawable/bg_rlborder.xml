<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/colorAccent"/>
        </shape>
    </item>
    <item
        android:right="1dp"
        android:left="1dp">
        <shape android:shape="rectangle">
            <solid
                android:color="#f0f0f0"/>
        </shape>
    </item>
</layer-list>